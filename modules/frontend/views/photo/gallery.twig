{% extends '@frontend/views/layouts/main.twig' %}

{% block scriptFiles %}
    {{ parent() }}
    <script src="{{ '/frontend/js/pages/gallery.js' }}" type="text/javascript"></script>
{% endblock %}

{% block content %}

    {% for photo in photos %}
        <div id="escort-photo-{{ photo.id }}" class="escort-photo auto-url" data-url="{{ path('photo/index', {'id': photo.id, 'escortId': escortId}) }}">
            <img src="{{ photo.src }}" height="200px" />
        </div>
    {% endfor %}

{% endblock %}

{% block scripts %}
    {{ parent() }}
    Gallery.init();
{% endblock %}